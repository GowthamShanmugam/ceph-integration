etcd_port: 2379
etcd_connection: 0.0.0.0
tendrl_ansible_exec_file: $HOME/.tendrl/node-agent/ansible_exec
log_cfg_path: /etc/tendrl/ceph-integration/ceph-integration_logging.yaml
log_level: DEBUG
logging_socket_path: /var/run/tendrl/message.sock

# This flag turns on internal profiling and saves stats at /var/lib/tendrl/profiling/ceph_integration/ on service stop  (Note: profiling requires "pip install GreenletProfiler")

with_internal_profiling: False
